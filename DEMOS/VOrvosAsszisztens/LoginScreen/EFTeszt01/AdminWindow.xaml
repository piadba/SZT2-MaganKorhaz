        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EFTeszt01"
        mc:Ignorable="d"
        Title="AdminWindow" Height="300" Width="456.442">
    <Grid>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="189" Margin="10,35,0,0" VerticalAlignment="Top" Width="428" IsSynchronizedWithCurrentItem="True" CanUserReorderColumns="False" CanUserSortColumns="False"/>
