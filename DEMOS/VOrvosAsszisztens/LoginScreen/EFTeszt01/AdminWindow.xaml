        <Button x:Name="modositasButton" Content="Tábla módosítás" HorizontalAlignment="Left" Height="20" Margin="243,10,0,0" VerticalAlignment="Top" Width="101" Click="modositasButton_Click"/>
        <Button x:Name="mentesButton" Content="Változások mentése" HorizontalAlignment="Left" Height="30" Margin="320,229,0,0" VerticalAlignment="Top" Width="118" Click="mentesButton_Click"/>

    </Grid>
</Window>
